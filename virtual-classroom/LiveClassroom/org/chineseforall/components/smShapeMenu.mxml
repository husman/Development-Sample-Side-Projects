<?xml version="1.0" encoding="utf-8"?>
<s:BorderContainer xmlns:fx="http://ns.adobe.com/mxml/2009"
				   xmlns:s="library://ns.adobe.com/flex/spark"
				   xmlns:mx="library://ns.adobe.com/flex/mx"
				   width="118" height="172" borderAlpha="0.9" borderColor="#FFAA16" cornerRadius="8"
				   dropShadowVisible="true">
	<s:layout>
		<s:TileLayout columnWidth="118" horizontalAlign="center" horizontalGap="0" orientation="rows"
					  requestedColumnCount="1" requestedRowCount="1" rowHeight="172"
					  verticalAlign="middle" verticalGap="0"/>
	</s:layout>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<s:VGroup x="96" y="53" width="100" height="160" gap="3">
		<s:HGroup width="100%" height="24" verticalAlign="middle"
				  id="tool_shapes_ellipse"
				  rollOver="tool_shapes_ellipse_label.styleName = 'lblToolTextHover';"
				  rollOut="tool_shapes_ellipse_label.styleName = 'lblToolText';">
			<s:Image id="tool_shapes_ellipse_bullet" source="@Embed('assets/images/tools/selections/sel_bullet.png')"
					 visible="false"/>
			<s:Image source="@Embed('assets/images/tools/selections/sel_shapeEllipse.png')"/>
			<s:Label id="tool_shapes_ellipse_label" styleName="lblToolText" text="Ellipse"/>
		</s:HGroup>
		<s:HGroup width="100%" height="24" verticalAlign="middle"
				  id="tool_shapes_rectangle"
				  rollOver="tool_shapes_rectangle_label.styleName = 'lblToolTextHover';"
				  rollOut="tool_shapes_rectangle_label.styleName = 'lblToolText';">
			<s:Image id="tool_shapes_rectangle_bullet" source="@Embed('assets/images/tools/selections/sel_bullet.png')"
					 visible="false" />
			<s:Image source="@Embed('assets/images/tools/selections/sel_shapeRectangle.png')"/>
			<s:Label id="tool_shapes_rectangle_label" styleName="lblToolText" text="Rectangle"/>
		</s:HGroup>
		<s:HGroup width="100%" height="24" verticalAlign="middle"
				  id="tool_shapes_triangle"
				  rollOver="tool_shapes_triangle_label.styleName = 'lblToolTextHover';"
				  rollOut="tool_shapes_triangle_label.styleName = 'lblToolText';">
			<s:Image id="tool_shapes_triangle_bullet" source="@Embed('assets/images/tools/selections/sel_bullet.png')"
					 visible="false" />
			<s:Image source="@Embed('assets/images/tools/selections/sel_shapeTriangle.png')"/>
			<s:Label id="tool_shapes_triangle_label" styleName="lblToolText" text="Triangle"/>
		</s:HGroup>
		<s:HGroup width="100%" height="24" verticalAlign="middle"
				  id="tool_shapes_line"
				  rollOver="tool_shapes_line_label.styleName = 'lblToolTextHover';"
				  rollOut="tool_shapes_line_label.styleName = 'lblToolText';">
			<s:Image id="tool_shapes_line_bullet" source="@Embed('assets/images/tools/selections/sel_bullet.png')"
					 visible="false" />
			<s:Image source="@Embed('assets/images/tools/selections/sel_shapeLine.png')"/>
			<s:Label id="tool_shapes_line_label" styleName="lblToolText" text="Lines"/>
		</s:HGroup>
		<s:HGroup width="100%" height="24" verticalAlign="middle"
				  id="tool_shapes_arrow"
				  rollOver="tool_shapes_arrow_label.styleName = 'lblToolTextHover';"
				  rollOut="tool_shapes_arrow_label.styleName = 'lblToolText';">
			<s:Image id="tool_shapes_arrow_bullet" source="@Embed('assets/images/tools/selections/sel_bullet.png')"
					 visible="false" />
			<s:Image source="@Embed('assets/images/tools/selections/sel_shapeArrow.png')"/>
			<s:Label id="tool_shapes_arrow_label" styleName="lblToolText" text="Arrows"/>
		</s:HGroup>
		<s:HGroup width="100%" height="24" verticalAlign="middle"
				  id="tool_shapes_bubble"
				  rollOver="tool_shapes_bubble_label.styleName = 'lblToolTextHover';"
				  rollOut="tool_shapes_bubble_label.styleName = 'lblToolText';">
			<s:Image id="tool_shapes_bubble_bullet" source="@Embed('assets/images/tools/selections/sel_bullet.png')"
					 visible="false" />
			<s:Image source="@Embed('assets/images/tools/selections/sel_shapeBubble.png')"/>
			<s:Label id="tool_shapes_bubble_label" styleName="lblToolText" text="Bubbles"/>
		</s:HGroup>
	</s:VGroup>
	
</s:BorderContainer>
